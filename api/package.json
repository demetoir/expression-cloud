	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" ",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "pm2-runtime ecosystem.config.jsy",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
		"typeorm:migration:create": "yarn typeorm migration:create -n",
		"typeorm:migration:run": "yarn typeorm migration:run",
		"typeorm:migration:revert": "yarn typeorm migration:revert",
		"generate:test-storage:migration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config test/util/typeorm/migration.config.js migration:generate -n prepare-schema",
		"docker:test-storage:up": "sh ./script/docker-test-storage-up.sh",
		"docker:test-storage:down": "sh ./script/docker-test-storage-down.sh",
		"docker:dev-storage:up": "sh script/docker-dev-storage-up.sh",
		"docker:dev-storage:down": "sh script/docker-dev-storage-down.sh",
		"docker:api:build": "sh ./script/docker-api-build.sh",
		"docker:api:up": "sh ./script/docker-api-up.sh",
		"docker:api:down": "sh ./script/docker-api-down.sh",
		"serverless:offline": "sls offline",
		"serverless:deploy": "sh ./script/deploy-serverless.sh",
		"compdoc": "compodoc -p tsconfig.json -s -d ./compdoc"
	},
