{
  "name": "expression-cloud-api",
  "version": "0.0.1",
  "description": "",
  "author": "demetoir",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" ",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pm2-runtime ecosystem.config.jsy",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:create": "yarn typeorm migration:create -n",
    "typeorm:migration:run": "yarn typeorm migration:run",
    "typeorm:migration:revert": "yarn typeorm migration:revert",
    "docker:test-storage:up": "sh ./script/docker-test-storage-up.sh",
    "docker:test-storage:down": "sh ./script/docker-test-storage-down.sh",
    "docker:storage:up": "sh ./script/docker-storage-up.sh",
    "docker:storage:down": "sh ./script/docker-storage-down.sh",
    "docker:api:build": "sh ./script/docker-api-build.sh",
    "docker:api:up": "sh ./script/docker-api-up.sh",
    "docker:api:down": "sh ./script/docker-api-down.sh",
    "serverless:offline": "sls offline",
    "serverless:deploy": "sh ./script/deploy-serverless.sh",
    "compdoc": "compodoc -p tsconfig.json -s -d ./compdoc"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.4",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.2",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/typeorm": "^7.1.0",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "@types/bcrypt": "^3.0.0",
    "@types/mongoose": "^5.7.36",
    "aws-lambda": "^1.0.6",
    "aws-serverless-express": "^3.3.8",
    "bcrypt": "^5.0.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "expect-ct": "^1.0.0",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mongoose": "^5.10.0",
    "morgan": "^1.10.0",
    "mysql-error-codes": "^0.1.2",
    "mysql2": "^2.1.0",
    "nest-morgan": "^1.0.1",
    "nest-winston": "^1.3.5",
    "nestjs-redoc": "^1.3.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rotating-file-stream": "^2.1.3",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.1",
    "@nestjs/testing": "^7.4.4",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.3",
    "@types/express-rate-limit": "^5.1.0",
    "@types/faker": "^5.1.0",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.159",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.4",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "chai": "^4.2.0",
    "eslint": "7.8.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.22.0",
    "faker": "^5.1.0",
    "jest": "26.4.2",
    "jest-mock-extended": "^1.0.9",
    "lint-staged": "^10.3.0",
    "node-gyp": "^7.1.0",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "serverless": "^1.82.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-layers": "^2.2.4",
    "serverless-offline": "^6.7.0",
    "serverless-plugin-include-dependencies": "^4.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  },
  "lint-staged": {
    "{src,apps,libs,test}/**/*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "./src",
      "./test"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
